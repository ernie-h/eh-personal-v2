@font-face{font-family:roboto;src:url(../font//Roboto-Bold.woff2) format("woff2"),url(../font//Roboto-Bold.woff) format("woff");font-weight:700;font-style:normal;font-display:fallback}@font-face{font-family:roboto;src:url(../font//Roboto-Italic.woff2) format("woff2"),url(../font//Roboto-Italic.woff) format("woff");font-weight:400;font-style:italic;font-display:fallback}@font-face{font-family:roboto;src:url(../font//Roboto-Regular.woff2) format("woff2"),url(../font//Roboto-Regular.woff) format("woff");font-weight:400;font-style:normal;font-display:fallback}:root{--mainCol: #20716a;--mainCol--light: #23a393;--secCol: #ffc0c2;--secCol--light: #f7e9e3;--linGrad-1: linear-gradient(to top right, var(--mainCol), var(--mainCol--light));--linGrad-webkit-1: -webkit-linear-gradient(top left, #183661, #1c4b82);--linGrad-2: linear-gradient(to top right, var(--secCol), var(--secCol--light));--gray: #dae1e7;--white: #ffffff;--font: "Roboto", sans-serif}html,body{margin:0 auto;font-family:var(--font);font-size:16px;background-color:var(--white);color:var(--mainCol)}h1,h2,h3,h4,h5{margin:0 auto;font-weight:400;line-height:1.25em}p{line-height:1.75em}a{text-decoration:none;color:var(--mainCol--light)}a:hover{color:var(--mainCol)}figure{margin:0}main{width:80%;max-width:710px;margin:4rem auto;word-wrap:break-word;min-height:70vh}section{margin:5em 0}.FooterNav{-webkit-box-shadow:2px 9px 25px 6px #777;-moz-box-shadow:2px 9px 25px 6px #777;box-shadow:2px 9px 25px 6px #777;background:var(--linGrad-2);text-transform:lowercase}.FooterNav :first-child{margin-left:10%}.FooterNav a{color:var(--mainCol--light)}.FooterNav a:hover{color:var(--mainCol)}.FooterNav :last-child{margin-right:10%}.FooterNav-Social{display:inherit}.FooterNav-Social svg{width:36px;height:36px;fill:var(--mainCol--light);transition:.2s ease-in-out}.FooterNav-Social svg:hover{transform:scale(1.2);fill:var(--mainCol)}.Skills{background-color:var(--white)}.SkillsCard{margin:1.75em 0;color:var(--mainCol--light)}.SkillsCard-Heading h3{background:var(--secCol);-webkit-background-clip:text;-webkit-text-fill-color:transparent;text-transform:uppercase;font-weight:700}.SkillsCard ul{line-height:1.75em;margin-left:.75em;border-left:solid 1px var(--gray)}.SkillsCard li{margin-bottom:16px}.Content{margin:4em 0}.Content h1{font-size:2.5rem;font-family:Georgia,Cambria,times new roman,Times,serif;letter-spacing:em;line-height:1.25em}.Content-Date{color:gray;font-style:italic;margin-top:1em}.Content code{}.Content figure{margin-top:2em}.Content figcaption{color:gray;font-style:italic;font-size:.75rem}.Content .highlight pre{padding:2.5em;border-radius:4px}.Content img{max-width:710px;max-height:540px}.Content hr{margin:2em auto}.ContentList{padding:0;list-style:none}.ContentList li{border-left:solid 1px var(--gray);margin-top:2.5em;padding-left:1.25em}